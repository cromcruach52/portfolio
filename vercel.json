{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": { "cache-control": "public, max-age=0, must-revalidate" },
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}